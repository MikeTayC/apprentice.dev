{
    "config": {
        "modules": {
            "user": {
                "pool": "Lib",
                "dir": "User",
              "events": {
                "unmark_completed_course": {
                  "delete_user_completed_course": {
                    "class": "user/observer",
                    "method": "deleteUserCompletedCourseMap"
                  }
                },
                "mark_completed_course": {
                  "mark_user_completed_course": {
                    "class": "user/observer",
                    "method": "markUserCompletedCourseMap"
                  }
                },
                "user_save_after": {
                  "add_user_group_tag": {
                    "class": "tag/observer",
                    "method": "setNewUserTag"
                  }
                },
                "user_delete_after": {
                  "delete_all_user_tags": {
                    "class": "tag/observer",
                    "method": "deleteUserTags"
                  },
                  "delete_user_completed_course": {
                    "class": "tag/observer",
                    "method": "deleteAllUserCompletedCourseMap"
                  }
                },
                "user_edit_tags_after": {
                  "delete_all_user_tags": {
                    "class": "tag/observer",
                    "method": "deleteUserTags"
                  },
                  "add_new_tags_to_db" : {
                    "class": "tag/observer",
                    "method": "addNewTagsToDb"

                  },
                  "add_new_tags_to_user" : {
                    "class": "tag/observer",
                    "method": "addTagsToUser"
                  }
                },
                "user_load_after" : {
                  "set_total_lesson_count" : {
                    "class" : "tag/observer",
                    "method" : "setTotalLessonCount"
                  },
                  "set_user_progress" :{
                    "class" :"user/observer",
                    "method": "setUserProgress"
                  },
                  "set_user_incubation_time" : {
                    "class":"user/observer",
                    "method": "setUserIncubationTime"
                  }
                },
                "user_load_profile" : {
                  "load_all_lessons": {
                    "class":"lesson/observer",
                    "method":"setAllLessons"
                  },
                  "set_completed_courses" : {
                    "class": "user/observer",
                    "method": "setUserCompletedCourses"
                  },
                  "load_all_user_tags" :{
                    "class":"tag/observer",
                    "method": "setAllUserTags"
                  }
                }
              }
            }
        }
    }
}